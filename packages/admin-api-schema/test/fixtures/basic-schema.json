{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "person",
  "title": "person",
  "type": "object",
  "required": ["first_name", "last_name", "age"],
  "properties": {
    "first_name": {
      "type": "string"
    },
    "last_name": {
      "type": "string"
    },
    "age": {
      "type": "integer"
    }
  }
}
