    "rollup-plugin-node-resolve": "^4.0.0",
    "rollup-plugin-terser": "^4.0.1",
    "should": "13.2.3",
    "sinon": "7.1.1"
  },
  "dependencies": {
    "lodash-es": "^4.17.11"
